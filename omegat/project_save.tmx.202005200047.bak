      </tuv>
    </tu>
    <tu>
      <tuv lang="en-US">
        <seg>// You may set the number of competitors to any number between 1 and 30.</seg>
      </tuv>
      <tuv lang="nl" changeid="Olivier" changedate="20200519T214351Z" creationid="Olivier" creationdate="20200519T214344Z">
