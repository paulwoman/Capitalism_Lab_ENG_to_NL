    <tu>
      <tuv lang="en-US">
        <seg>// You may set the game starting year to any year between 1900 and 2100.</seg>
      </tuv>
      <tuv lang="nl" changeid="Olivier" changedate="20200519T214053Z" creationid="Olivier" creationdate="20200519T214053Z">
